	<parameter name="cedula" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
    e.estcedula,
    e.estnombre,
    e.estapellido,
    e.estdireccion,
    e.esttelefono,
    e.estsexo,
    c.curnombre
FROM 
    estudiantes e
INNER JOIN 
    matriculas m ON e.estcedula = m.estudiante
INNER JOIN 
    cursos c ON m.curso = c.curid
WHERE 
    e.estcedula = $P{cedula}]]>
	</queryString>
