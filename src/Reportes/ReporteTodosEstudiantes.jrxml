	<queryString>
		<![CDATA[SELECT 
  e.estcedula,
  e.estnombre,
  e.estapellido,
  c.curnombre
FROM matriculas m
JOIN estudiantes e ON TRIM(e.estcedula) = m.estudiante
JOIN cursos c      ON c.curid = m.curso
ORDER BY e.estapellido, e.estnombre;]]>
